/var/log/rsync/rsync.log {
	daily
	rotate 7
	compress
	delaycompress
	missingok
	notifempty
	size 20M
	create 644 root root
}

/var/log/rsync/error.log {
        daily
        rotate 7
        compress
        delaycompress
        missingok
        notifempty
        size 20M
        create 644 root root
}